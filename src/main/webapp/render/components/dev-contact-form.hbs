<div class="jsg-contact-wrapper">
    <form action="{{formAction.url}}" method="POST" class="jsg-contact-form" data-bsp-form>
        <div class="bsp-container">
            <div class="bsp-row">
                <div class="bsp-column-full">

                    <div class="jsg-contact-name">
                        {{#with name}}
                            {{render this}}
                        {{/with}}
                    </div>

                    <div class="jsg-contact-phone">
                        {{#with phoneNumber}}
                            {{render this}}
                        {{/with}}
                    </div>

                    <div class="jsg-contact-email">
                        {{#with emailAddress}}
                            {{render this}}
                        {{/with}}
                    </div>

                    <div class="jsg-contact-msg">
                        {{#with msgContent}}
                            {{render this}}
                        {{/with}}
                    </div>

                    <div class="jsg-contact-submit">
                        {{#with submit}}
                            {{render this}}
                        {{/with}}
                    </div>

                </div>
            </div>
        </div>
    </form>
</div>
